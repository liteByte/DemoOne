{
  "name": "litebyte-demo-1",
  "version": "1.0.0",
  "private": true,
  "homepage": "http://litebyte.us",
  "devDependencies": {
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.1",
    "react-scripts": "0.9.0"
  },
  "dependencies": {
    "history": "^2.1.2",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "material-design-icons": "^3.0.1",
    "material-ui": "^0.17.0",
    "random-string": "^0.1.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-js-pagination": "^2.0.2",
    "react-loading-spinner": "^1.0.12",
    "react-responsive-table": "^2.0.11",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-tap-event-plugin": "^2.0.1"
  },
  "scripts": {
    "build-css": "node-sass src/scss -o public/",
